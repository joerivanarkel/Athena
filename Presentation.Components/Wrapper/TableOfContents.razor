<div class="rounded-lg text-card-foreground shadow-lg bg-white border border-gray-200 mt-4" data-v0-t="card">
    <div class="flex flex-col space-y-1.5 bg-gray-100 border-b border-gray-200 p-4">
        <h3 class="tracking-tight text-2xl font-semibold text-gray-700">Table of Contents</h3>
    </div>
    <div class="space-y-2 text-gray-700 p-4">
        <ul class="list-disc pl-5">
            @ChildContent
        </ul>
    </div>
</div>

@code {
    [Parameter] 
    public RenderFragment ChildContent { get; set; }
}
